{
  "name": "navigate",
  "description": "Navigate to a URL in existing browser window/tab",
  "category": "browser",
  "parameters": [
    {
      "name": "url",
      "type": "string",
      "description": "URL to navigate to",
      "required": true,
      "example": "https://google.com"
    }
  ],
  "examples": [
    "navigate to google.com",
    "go to https://github.com",
    "przejdź do strony youtube.com"
  ],
  "patterns": [
    "navigate", "go to", "przejdź", "idź do", "wejdź na"
  ],
  "source_type": "browser",
  "metadata": {
    "playwright_action": "page.goto('{url}')"
  },
  "template": "page.goto('{url}')"
}
