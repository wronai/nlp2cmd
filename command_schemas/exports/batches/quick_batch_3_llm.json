            {
              "name": "-C",
              "type": "boolean",
              "description": "Shows the process name for each socket.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            },
            {
              "name": "-F",
              "type": "boolean",
              "description": "Shows the file descriptor number for each socket.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            },
            {
              "name": "<Af>",
              "type": "string",
              "description": "Specifies the address family to display (e.g., inet, unix).",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "argument"
            },
