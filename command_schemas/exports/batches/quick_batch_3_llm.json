    "wget": {
      "source_type": "llm_enhanced",
      "commands": [
        {
          "name": "wget",
          "description": "A utility for non-interactive download of files from the web.",
          "category": "network",
          "parameters": [
            {
              "name": "URL",
              "type": "string",
              "description": "The URL of the file to be downloaded.",
              "required": true,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "argument"
            },
            {
              "name": "-O, --output-document=FILE",
              "type": "string",
              "description": "Write output to FILE instead of stdout.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            },
            {
              "name": "-q, --quiet",
              "type": "boolean",
              "description": "Work silently (background mode).",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            }
          ],
          "examples": [
            "wget http://example.com/file.zip",
            "wget -O myfile.txt http://example.com/textfile.txt",
            "wget -q http://example.com/image.jpg"
          ],
          "patterns": [
            "wget",
            "a utility for non-interactive download of files from the web."
          ],
          "source_type": "llm_enhanced",
          "metadata": {
            "llm_model": "ollama/qwen2.5-coder:7b",
            "enhanced": true
          },
          "template": "wget {URL} [-O {output}] [-q]"
        }
      ],
