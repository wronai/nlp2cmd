    "find": {
      "source_type": "llm_enhanced",
      "commands": [
        {
          "name": "find",
          "description": "The find command is used to search for files and directories within a directory hierarchy.",
          "category": "file",
          "parameters": [
            {
              "name": "path",
              "type": "string",
              "description": "The starting point of the search. Defaults to the current directory if not specified.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "argument"
            },
            {
              "name": "-H",
              "type": "boolean",
              "description": "Follow symbolic links on command line only.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            },
            {
              "name": "-L",
              "type": "boolean",
              "description": "Follow all symbolic links.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            },
            {
              "name": "-P",
              "type": "boolean",
              "description": "Do not follow any symbolic links.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            },
            {
              "name": "-Olevel",
              "type": "string",
              "description": "Optimization level for the search algorithm.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            },
            {
              "name": "-D debugopts",
              "type": "string",
              "description": "Debugging options to help diagnose issues with the find command.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            }
          ],
          "examples": [
            "find /home/user -name '*.txt'",
            "find /var/log -type f -size +10M",
            "find . -mtime -7 -exec rm {} \\;"
          ],
          "patterns": [
            "find",
            "the find command is used to search for files and directories within a directory hierarchy."
          ],
          "source_type": "llm_enhanced",
          "metadata": {
            "llm_model": "ollama/qwen2.5-coder:7b",
            "enhanced": true
          },
          "template": "find {path} -name \"{pattern}\""
        }
      ],
      "metadata": {
        "model": "ollama/qwen2.5-coder:7b"
      }
