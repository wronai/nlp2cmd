    "find": {
      "source_type": "llm_enhanced",
      "commands": [
        {
          "name": "find",
          "description": "The find command is used to search for files and directories within a directory hierarchy.",
          "category": "file",
          "parameters": [
            {
              "name": "path",
              "type": "string",
              "description": "The starting path from which the search begins. If not specified, the current directory is used.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "argument"
            },
            {
              "name": "expression",
              "type": "string",
              "description": "A set of conditions to match files and directories. Common options include -type (file type), -size (file size), -mtime (modification time).",
              "required": true,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "argument"
            },
            {
              "name": "-H",
              "type": "boolean",
              "description": "Follow symbolic links if they lead to directories.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            },
            {
              "name": "-L",
              "type": "boolean",
              "description": "Follow all symbolic links.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            },
            {
              "name": "-P",
              "type": "boolean",
              "description": "Do not follow symbolic links.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            },
            {
              "name": "-Olevel",
              "type": "string",
              "description": "Set optimization level for the search. Higher levels can speed up the search but may use more memory.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            },
            {
              "name": "-D debugopts",
              "type": "string",
              "description": "Enable debugging options to help diagnose issues with the find command.",
              "required": false,
              "default": null,
              "choices": [],
              "pattern": null,
              "location": "option"
            }
          ],
          "examples": [
            "find /home/user -type f -size +10M",
            "find /var/log -mtime -7 -name '*.log'",
            "find . -type d -empty"
          ],
          "patterns": [
            "find",
            "the find command is used to search for files and directories within a directory hierarchy."
          ],
          "source_type": "llm_enhanced",
          "metadata": {
            "llm_model": "ollama/qwen2.5-coder:7b",
            "enhanced": true
          },
          "template": "find {path} -type {type} -{option}"
        }
      ],
      "metadata": {
        "model": "ollama/qwen2.5-coder:7b"
      }
    },
